<html>
<head>
	<title></title>
	<meta charset="utf-8">


	<!-- 最新 Bootstrap 核心 CSS 文件 -->
	<link rel="stylesheet" href="{{ static_url("flat-ui/bootstrap/css/bootstrap.css") }}">

	<!-- jQuery文件。务必在bootstrap.min.js 之前引入 -->
    <script src="{{ static_url("script/jquery-latest.js") }}"></script>
    <script src="{{ static_url("script/jquery.cookie.js") }}"></script>

	<!-- 最新的 Bootstrap 核心 JavaScript 文件 -->
    <script src="{{ static_url("bootstrap/js/bootstrap.min.js") }}"></script>
	<link rel="stylesheet" href="{{ static_url("bootstrap/css/bootstrap-responsive.min.css") }}">

    <script src="{{ static_url("script/signup.js") }}"></script>

	<style>
	</style>
</head>
<body>
	<div class="container-fluid">
		<div class="row-fluid">
			<div class="span2">
			</div>

			<div class="span6">


			  <form class="form-horizontal">
			    <fieldset>
			      <div id="legend" class="">
			        <legend class="">用户注册</legend>
			      </div>
			    <div class="control-group">
			
			          <!-- Email input-->
			          <label class="control-label" for="input01">邮箱</label>
			          <div class="controls">
			            <input type="text" id="email-address" placeholder="输入你的邮箱地址" class="input-xlarge">
			            <p class="help-block"></p>
			          </div>
			        </div>
			
			    <div class="control-group">
			          <!-- Nickname input-->
			          <label class="control-label" for="input01">昵称</label>
			          <div class="controls">
			            <input type="text" id="nick-name" placeholder="输入昵称" class="input-xlarge">
			            <p class="help-block"></p>
			          </div>
			        </div>
			    
			
			    <div class="control-group">
			
			          <!-- Text input-->
			          <label class="control-label">密码</label>
			          <div class="controls">
			            <input type="password" id="password" placeholder="输入密码" class="input-xlarge">
			            <p class="help-block">推荐8-16位字母、数字组合</p>
			          </div>
			
			        </div><div class="control-group">
			
			          <!-- Text input-->
			          <label class="control-label" for="input01">确认密码</label>
			          <div class="controls">
			            <input type="password" id="password-confirmation" placeholder="请再输入一次密码" class="input-xlarge">
			            <p class="help-block"></p>
			          </div>
			        </div>
			
			    <div class="control-group">
			          <label class="control-label"></label>
			          <div class="controls">
			      <!-- Multiple Checkboxes -->
			      <label class="checkbox">
			        <input type="checkbox" id="is_subscribed" value="邮件接收软件学院学生周知新闻" checked>
			        邮件接收软件学院学生周知新闻
			      </label>
			  </div>
			
			        </div><div class="control-group">
			          <label class="control-label"></label>
			
			          <!-- Button -->
			          <div class="controls">
			            <button class="btn btn-success" id="submit">完成注册</button>
			          </div>
			        </div>
			    <fieldset>
			  </form>
			</div>
			<div class="span4">
			</div>
		</div>
	</div>
</body>

